.sd-deck {
  position: relative;
  margin-top: 1.5rem;
  margin-left: auto;
  margin-right: auto;
  width: calc(25 * 1.5rem);
  height: calc(20 * 1.5rem);
  overflow: hidden;
	border-bottom: 1px solid rgba(0, 0, 0, 0.2);
	box-shadow: 0 1px 5px rgba(0, 0, 0, 0.15);
  border-top: 1.5rem solid #999;
  border-bottom: 1.5rem solid #999;
  background: #ddd;
}

.sd-card-slider {
  display: block;
  height: 100%;
}

.sd-card {
  float: left;
  min-height: 100%;
  height: 100%;
  background: white;
}

.sd-card-gripper {
  display: block;
  position: relative;
  background-color: #bbb;
  width: 1.5rem;
  height: 100%;
  float: left;
}

.sd-card-gripper:nth-child(2) {
  float: right;
}

.sd-card-gripper:hover {
  background-color: #ccc;
}

.sd-card-gripper::after {
  display: block;
  position: absolute;
  content: "â¬Œ";
  width: 100%;
  text-align: center;
  top: 50%;
  margin-top: -50%;
  color: #444;
}
